{
  "manifest_version": 3,
  "name": "Export Browsing History",
  "version": "1.0",
  "description": "Enable users to export their browsing history in various formats.",

  "action": {
    "default_popup": "history.html",
    "default_icon": {
      "16": "export.png",
      "32": "export.png",
      "48": "export.png",
      "128": "export.png"
    },
    "default_title": "Export Browsing History" 
  },
  "icons": {
    "16": "export.png",
    "32": "export.png",
    "48": "export.png",
    "128": "export.png"
    },
  "permissions": [
    "history",
    "downloads","storage"
  ]
 

}